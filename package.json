{
  "name": "dx-react-devextreme-data-server",
  "version": "1.0.17",
  "description": "DevExtreme React Grid plugin for remote data loading from a DevExtreme data service",
  "repository": {
    "type": "git",
    "url": "https://github.com/oliversturm/dx-react-devextreme-data-server.git"
  },
  "author": "Oliver Sturm",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/oliversturm/dx-react-devextreme-data-server/issues"
  },
  "homepage": "https://github.com/oliversturm/dx-react-devextreme-data-server",
  "keywords": [
    "devextreme",
    "devexpress",
    "react",
    "grid"
  ],
  "options": {
    "mocha": "--require scripts/mocha_runner src/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepare": "babel src --ignore __tests__ --out-dir ./dist && copyfiles -u 1 ./src/loading.css ./dist && webpack",
    "lint": "eslint ./src",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js"
  },
  "devDependencies": {
    "@devexpress/dx-react-core": "^1.0.0-alpha.14",
    "@devexpress/dx-react-grid": "^1.0.0-alpha.14",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-remove-comments": "^2.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^4.1.2",
    "copyfiles": "^1.2.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "eslint": "^4.10.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-react": "^7.4.0",
    "jsdom": "^11.3.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.0.0",
    "sinon": "^4.0.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "@devexpress/dx-react-core": "1.0.0-alpha.14",
    "@devexpress/dx-react-grid": "^1.0.0-alpha.14",
    "prop-types": "^15.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  }
}
